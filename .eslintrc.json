{
	"env": {
		"browser": true,
		"es2021": true,
		"jest": true
	},
	"extends": ["eslint:recommended", "plugin:react/recommended", "plugin:react-hooks/recommended"],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"plugins": ["react", "react-hooks", "prettier"],
	"settings": {
		"react": {
			"version": "detect"
		}
	}
}
